    "demo": "owc-dev-server --app-index ./demo/demo.html --open --watch"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "cross-env": "^5.2.0",
    "del-cli": "^1.1.0",
    "owc-dev-server": "^0.4.3",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "lit-element": "^2.1.0"
  }
}
