{
  "name": "lit-button",
  "version": "2.2.1",
  "description": "Dead simple lit-element button",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/lit-button",
  "bugs": "https://github.com/mgenware/lit-button/issues",
  "keywords": [
    "lit-element"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/lit-button.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint",
    "build": "cross-env NODE_ENV=production yarn test",
    "compile": "tsc -b src",
    "dev": "cross-env NODE_ENV=development yarn run clean && tsc -b src -w",
    "prepublishOnly": "yarn run test",
    "lint": "eslint --ext .ts src/ && lit-analyzer \"src/**/*.ts\" --strict",
    "clean": "del-cli dist",
    "demo": "owc-dev-server --app-index ./demo/demo.html --open --watch"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.7",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "cross-env": "^6.0.3",
    "del-cli": "^3.0.0",
    "eslint": "^6.6.0",
    "lit-analyzer": "^1.1.9",
    "lit-element": "^2.2.1",
    "owc-dev-server": "^1.0.1",
    "typescript": "^3.7.2"
  }
}
