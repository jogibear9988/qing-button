{
  "name": "lit-button",
  "version": "3.3.0",
  "description": "Dead simple lit-element button",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/lit-button",
  "bugs": "https://github.com/mgenware/lit-button/issues",
  "keywords": [
    "lit-element"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/lit-button.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint && yarn run t",
    "t": "karma start",
    "build": "cross-env NODE_ENV=production yarn test",
    "compile": "tsc -b tests",
    "dev": "cross-env NODE_ENV=development yarn run clean && tsc -b tests -w",
    "prepublishOnly": "yarn run test",
    "lint": "eslint --ext .ts src/ tests/ && lit-analyzer \"src/**/*.ts\" --strict",
    "clean": "del-cli dist",
    "demo": "owc-dev-server --app-index ./demo/demo.html --open --watch"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/dist/**/*.js",
    "/dist/**/*.ts",
    "/dist/**/*.map"
  ],
  "devDependencies": {
    "@open-wc/testing": "^2.5.2",
    "@open-wc/testing-karma": "^3.2.33",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "cross-env": "^7.0.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "karma": "^4.4.1",
    "lit-analyzer": "^1.1.9",
    "lit-element": "^2.2.1",
    "owc-dev-server": "^1.0.1",
    "typescript": "^3.7.5",
    "webpack-merge": "^4.2.2"
  }
}
